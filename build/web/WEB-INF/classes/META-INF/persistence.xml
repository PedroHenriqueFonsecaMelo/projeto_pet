<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="projeto_petPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>DAOS.Parameters</class>
    <class>DAOS.Columns</class>
    <class>DAOS.DomainConstraints</class>
    <class>DAOS.ReferentialConstraints</class>
    <class>DAOS.KeyColumnUsage</class>
    <class>DAOS.ColumnPrivileges</class>
    <class>DAOS.Routines</class>
    <class>DAOS.CheckConstraints</class>
    <class>DAOS.Fields</class>
    <class>DAOS.Triggers</class>
    <class>DAOS.TableConstraints</class>
    <class>DAOS.ConstraintColumnUsage</class>
    <class>DAOS.Sequences</class>
    <class>DAOS.Tables</class>
    <class>DAOS.Schemata</class>
    <class>DAOS.Views</class>
    <class>DAOS.TablePrivileges</class>
    <class>DAOS.Collations</class>
    <class>DAOS.Domains</class>
    <class>DAOS.ElementTypes</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:h2:tcp://localhost/~/test;MODE=LEGACY"/>
      <property name="javax.persistence.jdbc.user" value="sa"/>
      <property name="javax.persistence.jdbc.driver" value="org.h2.Driver"/>
      <property name="javax.persistence.jdbc.password" value="password"/>
    </properties>
  </persistence-unit>
</persistence>
